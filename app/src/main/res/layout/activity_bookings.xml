<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Bookings">

  <Spinner
      android:id="@+id/spinner1"
      android:layout_width="154dp"
      android:layout_height="33dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.902"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.637" />


  <TextView
      android:id="@+id/textView2"
      android:layout_width="136dp"
      android:layout_height="41dp"
      android:text="Book Now"
      android:textColor="#0C0C0C"
      android:textSize="28dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.56"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.023" />

  <TextView
      android:id="@+id/textView24"
      android:layout_width="84dp"
      android:layout_height="35dp"
      android:text="Date"
      android:textSize="20sp"
      android:textStyle="bold"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.048"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.438" />

  <TextView
      android:id="@+id/textView7"
      android:layout_width="84dp"
      android:layout_height="35dp"
      android:text="From"
      android:textSize="20sp"
      android:textStyle="bold"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.048"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.196" />

  <TextView
      android:id="@+id/textView8"
      android:layout_width="84dp"
      android:layout_height="35dp"
      android:text="To"
      android:textSize="20sp"
      android:textStyle="bold"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.048"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.29" />

  <TextView
      android:id="@+id/textView25"
      android:layout_width="84dp"
      android:layout_height="35dp"
      android:text="Time"

      android:textSize="20sp"
      android:textStyle="bold"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.048"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.55" />

  <TextView
      android:id="@+id/textView26"
      android:layout_width="84dp"
      android:layout_height="35dp"
      android:text="Category"
      android:textSize="20sp"
      android:textStyle="bold"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.048"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.639" />

  <EditText
      android:id="@+id/Username"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:ems="10"
      android:hint="          Username"
      android:inputType="text"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.87"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.749" />

  <TextView
      android:id="@+id/textView32"
      android:layout_width="84dp"
      android:layout_height="35dp"
      android:text="Username"
      android:textSize="20sp"
      android:textStyle="bold"

      android:clickable="false"
      android:cursorVisible="false"
      android:focusable="false"
      android:focusableInTouchMode="false"

      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.048"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.738" />

  <TextView
      android:id="@+id/textView36"
      android:layout_width="84dp"
      android:layout_height="35dp"
      android:text="QTY"
      android:textSize="20sp"
      android:textStyle="bold"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.048"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.826" />

  <Spinner
      android:id="@+id/spinner"
      android:layout_width="154dp"
      android:layout_height="33dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.898"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.826" />

  <Spinner
      android:id="@+id/spinner2"
      android:layout_width="154dp"
      android:layout_height="33dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.941"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.289" />

  <Spinner
      android:id="@+id/spinner3"
      android:layout_width="154dp"
      android:layout_height="33dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.937"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.194" />



  <Button
      android:id="@+id/btn_pr_bk"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:onClick="Proceed"
      android:text="Proceed"
      app:backgroundTint="#A81C1F37"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.907" />

  <Button
      android:id="@+id/getdateBtn"
      android:layout_width="106dp"
      android:layout_height="44dp"
      android:onClick="GetDate"
      android:text="GetDate"
      android:textAllCaps="false"
      app:backgroundTint="#2A587A"
      app:layout_constraintBaseline_toBaselineOf="@+id/textView24"
      app:layout_constraintEnd_toEndOf="@+id/editTextDate"
      app:layout_constraintHorizontal_bias="1.0"
      app:layout_constraintStart_toStartOf="@+id/editTextDate" />

  <Button
      android:id="@+id/getdateBtn2"
      android:layout_width="106dp"
      android:layout_height="44dp"
      android:onClick="GetDate"
      android:text="Get Date"
      android:textAllCaps="false"
      app:backgroundTint="#2A587A"
      app:layout_constraintBaseline_toBaselineOf="@+id/textView24"
      app:layout_constraintEnd_toEndOf="@+id/editTextDate"
      app:layout_constraintHorizontal_bias="1.0"
      app:layout_constraintStart_toStartOf="@+id/editTextDate" />

  <Button
      android:id="@+id/getTime"
      android:layout_width="106dp"
      android:layout_height="44dp"
      android:onClick="GetTime"
      android:text="Get Time"
      android:textAllCaps="false"
      app:backgroundTint="#2A587A"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.842"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.557" />

  <TextView
      android:id="@+id/editTextTime"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Current date"
      android:textStyle="bold"
      app:layout_constraintBottom_toTopOf="@+id/getTime"
      app:layout_constraintEnd_toEndOf="@+id/getTime"
      app:layout_constraintHorizontal_bias="1.0"
      app:layout_constraintStart_toStartOf="@+id/getTime"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="1.0" />

  <TextView
      android:id="@+id/editTextDate"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Current date"
      android:textStyle="bold"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.815"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.389" />

</androidx.constraintlayout.widget.ConstraintLayout>