<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cbb.transportcbb">

    <application
        android:allowBackup="true"
        android:icon="@drawable/book"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_launcher_background"
        android:supportsRtl="true"
        android:theme="@style/Theme.TransportCBB">

        <activity android:name=".UserProfile"></activity>
        <activity android:name=".BookingSuccess" />
        <activity
            android:name=".CardActivity"
            android:exported="true"
            android:parentActivityName=".BookingSuccess">
        </activity>
        <activity
            android:name=".PaymentSuccessActivity"
            android:exported="true"></activity>
        <activity
            android:name=".ManageBusTime"
            android:exported="true"
            android:parentActivityName=".home"></activity>
        <activity
            android:name=".AddTimeDetails"
            android:exported="true"
            android:parentActivityName=".ManageBusTime"></activity>
        <activity
            android:name=".HireActivity"
            android:exported="true"
            android:parentActivityName=".home"></activity>
        <activity
            android:name=".RetrieveCardActivity"
            android:exported="true"
            android:parentActivityName=".home"></activity>
        <activity
            android:name=".Bookings"
            android:parentActivityName=".home" />

        <activity
            android:name=".FeedbackActivity"
            android:exported="true"
            android:parentActivityName=".home"/>
        <activity
            android:name=".ShowFeedbackActivity"
            android:exported="true"
            android:parentActivityName=".home"/>
        <activity
            android:name=".ReviewActivity"
            android:exported="true"
            android:parentActivityName=".RetrieveCardActivity"/>
        <activity android:name=".home" />
        <activity android:name=".Signup" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>